{
"Logging": {
  "LevelSwitches": {
    "Default":
      {"Level": "Debug"},
    "Microsoft":
      {"Level": "Information"},
    "System":
      {"Level": "Information"},
    "ConsoleSink":
      {"Level": "Verbose"}
},
"FilterSwitches": {
  "ConsoleSink" : 
    {"Expression": "true"}
  },
  "Sinks": {
    "EventLog": {
      "LogPath": "logs",
      "LogName": "events.log.json",
      "LogSize": 10485760,
      "LogRetention": 7,
      "RollingInterval": "Day"
      },
      "DiagnosticLog": {
        "LogPath": "logs",
        "LogName": "diagnostics.log.json",
        "LogSize": 10485760,
        "LogRetention": 7,
        "RollingInterval": "Day"
      },
      "DebugLog": {
        "LogPath": "logs",
        "LogName": "debug.log",
        "LogSize": 10240,
        "LogRetention": 7,
        "RollingInterval": "Hour",
        "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Type} {RequestId} {SourceContext}] {Message}{NewLine}"
      }
    }
},
    "ConnectionStrings": {
      "(identityDb)": "DataSource=Identity.db"
    },
    "WeeWXRepository": {
      "Provider": "Sqlite",
      "ConnectionString": "DataSource=Data/Development/weather.db"
    },
    "WeatherApi": {
      "ConditionRepository": {
        "Provider": "Sqlite",
        "ConnectionString": "DataSource=Data/Development/weather.db",
        "MaximumObservations": 10
      },
      "ObservationRepository": {
        "Provider": "Sqlite",
        "ConnectionString": "DataSource=Data/Development/weather.db",
        "MaximumObservations": 10
      }
    }
}