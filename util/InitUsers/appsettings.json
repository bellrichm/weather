{
"Logging": {
  "LevelSwitches": {
    "Default":
      {"Level": "Debug"},
    "Microsoft":
      {"Level": "Information"},
    "System":
      {"Level": "Information"},
    "ConsoleSink":
      {"Level": "Verbose"}
},
"FilterSwitches": {
  "ConsoleSink" : 
    {"Expression": "true"}
  },
"Sinks": {
  "EventLog": {
    "LogPath": "logs",
    "LogName": "events.log.json",
    "LogSize": 10485760,
    "LogRetention": 7
    },
    "DiagnosticLog": {
      "LogPath": "logs",
      "LogName": "diagnostics.log.json",
      "LogSize": 10485760,
      "LogRetention": 7
    },
    "DebugLog": {
      "LogPath": "logs",
      "LogName": "debug.log",
      "LogSize": 10485760,
      "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Type} {RequestId}] {Message}{NewLine}"
    }
  }
},
    "ConnectionStrings": {
      "(identityDb)": "DataSource=Identity.db"
    },

  "Identity": {
    "JwtConfiguration": {
      "ValidFor": 1440,
      "Issuer": "issuer",
      "Audience": "audience"
    },
    "SecretKey": "UtilitySecretKey"
  }
}