{
    "Logging": {
      "LevelSwitches": {
        "Default":
          {"Level": "Information"},
        "Microsoft":
          {"Level": "Warning"},
        "System":
          {"Level": "Warning"},
        "ConsoleSink":
          {"Level": "Fatal"}
    },
    "FilterSwitches": {
      "ConsoleSink" :
        {"Expression": "false"}
      },
    "Sinks": {
      "EventLog": {
        "LogPath": "logs",
        "LogName": "events.log.json",
        "LogSize": 10485760,
        "LogRetention": 7,
        "RollingInterval": "Day"
        },
        "DiagnosticLog": {
          "LogPath": "logs",
          "LogName": "diagnostics.log.json",
          "LogSize": 10485760,
          "LogRetention": 7,
          "RollingInterval": "Day"
        },
        "DebugLog": {
          "LogPath": "logs",
          "LogName": "debug.log",
          "LogSize": 102400,
          "LogRetention": 7,
          "RollingInterval": "Hour",
          "OutputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} {Type} {RequestId} {SourceContext}] {Message}{NewLine}"
        }
      }
    },
    "ConnectionStrings": {
      "(identityDb)": "DataSource=Data/Identity.db"
    },
    "WeatherApi": {
      "Repository": {
        "Provider": "Sqlite",
        "ConnectionString": "DataSource=Data/weather.db",
        "MaximumConditions": 10
      }
    },
  "Identity": {
    "JwtConfiguration": {
      "ValidFor": 5,
      "Issuer": "issuer",
      "Audience": "audience"
    },
    "SecretKey": ""
  }
}
