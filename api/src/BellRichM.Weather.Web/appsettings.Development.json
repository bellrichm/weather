{
  "Logging": {
    "LevelSwitches": {
      "Default":
        {"Level": "Debug"},
      "Microsoft":
        {"Level": "Information"},
      "System":
        {"Level": "Information"},
      "ConsoleSink":
        {"Level": "Verbose"}
  },
  "FilterSwitches": {
    "ConsoleSink" :
      {"Expression": "true"}
    }
  },
    "ConnectionStrings": {
      "(identityDb)": "DataSource=Data/Development/Identity.db"
    },
  "Identity": {
    "JwtConfiguration": {
      "ValidFor": 1440
    }
  }
}